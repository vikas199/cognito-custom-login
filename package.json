{
  "name": "congito-login",
  "version": "0.1.0",
  "description": "Login app for CWDS-CARES",
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "preinstall": "node tools/nodeVersionCheck.js",
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm run start-message",
    "start": "ENV_PATH=./.env concurrently -k -r -s first \"npm run test:watch\" \"npm run open:src\"",
    "open:src": "babel-node tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "lint": "eslint --ext .jsx --ext .js src",
    "lint-fix": "eslint --ext .jsx --ext .js --fix src",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "prebuild": "npm run lint && npm run test && npm run clean-dist",
    "build": "babel-node tools/build.js && npm run open:dist",
    "build:integration": "ENV_PATH=./env/.integration.env babel-node tools/build.js && npm run open:dist",
    "build:sandboxcans": "ENV_PATH=./env/.sandboxcans.env babel-node tools/build.js",
    "build:staging": "ENV_PATH=./env/.staging.env babel-node tools/build.js && npm run open:dist",
    "build:training": "ENV_PATH=./env/.training.env babel-node tools/build.js && npm run open:dist",
    "build:demoint": "ENV_PATH=./env/.demoint.env babel-node tools/build.js && npm run open:dist",
    "build:performance": "ENV_PATH=./env/.performance.env babel-node tools/build.js && npm run open:dist",
    "build:preprod": "ENV_PATH=./env/.preprod.env babel-node tools/build.js && npm run open:dist",
    "build:production": "ENV_PATH=./env/.production.env babel-node tools/build.js && npm run open:dist",
    "license-dump": "license-checker --csv --out legal/licenses.csv",
    "test": "jest --detectOpenHandles",
    "test:CI": "babel-node tools/testCi.js",
    "test:cover": "npm run test -- --coverage ",
    "test:cover:CI": "npm run test:CI -- --coverage && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
    "test:watch": "jest --watch",
    "open:cover": "npm run test:cover && opn ./coverage/lcov-report/index.html",
    "analyze-bundle": "babel-node ./tools/analyzeBundle.js"
  },
  "author": "CWDS",
  "license": "AGPL 3.0",
  "dependencies": {
    "amazon-cognito-identity-js": "^2.0.0",
    "dotenv-webpack": "1.5.7",
    "object-assign": "4.1.0",
    "query-string": "5.1.1",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-router-dom": "4.2.2"
  },
  "devDependencies": {
    "autoprefixer": "7.1.4",
    "babel-cli": "6.24.1",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.1",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-polyfill": "6.23.0",
    "babel-preset-env": "1.6.0",
    "babel-preset-react": "6.24.1",
    "browser-sync": "2.18.12",
    "chalk": "2.0.1",
    "concurrently": "3.5.0",
    "connect-history-api-fallback": "1.3.0",
    "coveralls": "2.13.1",
    "css-loader": "0.28.7",
    "enzyme": "3.0.0",
    "enzyme-adapter-react-16": "1.0.0",
    "eslint": "4.19.1",
    "eslint-config-standard": "11.0.0",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-node": "6.0.1",
    "eslint-plugin-promise": "3.8.0",
    "eslint-plugin-react": "7.10.0",
    "eslint-plugin-standard": "3.1.0",
    "eslint-watch": "3.1.2",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "file-loader": "1.1.11",
    "hard-source-webpack-plugin": "0.7.4",
    "history": "4.6.0",
    "html-webpack-plugin": "3.2.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "23.6.0",
    "jest-cli": "23.6.0",
    "jest-puppeteer": "3.4.0",
    "jsdom": "12.2.0",
    "jsdom-global": "3.0.2",
    "json-loader": "0.5.4",
    "mockdate": "2.0.1",
    "node": "8.9.4",
    "node-sass": "4.5.3",
    "opn-cli": "3.1.0",
    "postcss-loader": "2.0.6",
    "prompt": "1.0.0",
    "prop-types": "15.5.10",
    "puppeteer": "1.9.0",
    "raf": "3.3.2",
    "react-hot-loader": "4.0.1",
    "react-test-renderer": "16.0.0",
    "redux-immutable-state-invariant": "2.0.0",
    "redux-mock-store": "1.3.0",
    "replace": "0.3.0",
    "rimraf": "2.6.1",
    "sass-loader": "6.0.6",
    "style-loader": "0.21.0",
    "url-loader": "1.0.1",
    "webpack": "4.6.0",
    "webpack-bundle-analyzer": "2.11.1",
    "webpack-dev-middleware": "3.1.3",
    "webpack-hot-middleware": "2.22.1",
    "webpack-md5-hash": "0.0.6"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": ""
  },
  "jest": {	
    "moduleNameMapper": {	
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tools/assetsTransformer.js",	
      "\\.(css)$": "<rootDir>/tools/assetsTransformer.js"	
    },	
    "setupFiles": [	
      "raf/polyfill",	
      "./tools/enzymeTestAdapterSetup.js"	
    ],
    "preset": "jest-puppeteer"	
  }
}
